using System;

class MainClass {
  public static void Main (string[] args) {
    int päron_pris = 13;
    int äpplen_pris = 7;
    Console.WriteLine("Hur många äpplen har Axel sålt? ");
    string antal_äpplen = Console.ReadLine();
    int antal_äpplen1 = int.Parse(antal_äpplen);
    Console.WriteLine("Hur många päron har Petra sålt? ");
    string antal_päron = Console.ReadLine();
    int antal_päron1 = int.Parse(antal_päron);
int antal_kr_päron = antal_päron1 * päron_pris;
int antal_kr_äpplen = antal_äpplen1 * äpplen_pris;
if (antal_kr_päron > antal_kr_äpplen)
Console.WriteLine("Petra ");
else if (antal_kr_päron < antal_kr_äpplen)
Console.WriteLine("Axel ");
else
Console.WriteLine("lika ");
  }
}

using System;

class MainClass {
  public static void Main (string[] args) {
Console.WriteLine("Skriv in dina första 6 siffror i ditt personnummer ");
string personnummer = Console.ReadLine();
int personNummer = int.Parse(personnummer);
Console.WriteLine("Skriv in dina fyra sista siffror ");
string fyra_sista = Console.ReadLine();
int fyra_Sista = int.Parse(fyra_sista);
int hela_personnummer = personNummer + fyra_Sista;
if (hela_personnummer < 201127+9999);
Console.WriteLine("Du är född före 1920 "); 
if (hela_personnummer > 201127-9999)
Console.WriteLine("Du är född efter 1920 ");
  }
}
